# 建筑名称
msgctxt "STRINGS.BUILDINGS.PREFABS.BUILDABLENATURALTILEFIX_NATURALTILE.NAME"
msgid "Natural Tile"
msgstr "自然地砖"

# 建筑描述
msgctxt "STRINGS.BUILDINGS.PREFABS.BUILDABLENATURALTILEFIX_NATURALTILE.DESC"
msgid "Fill that hole you dug out back in with any solid element."
msgstr "用任何固体元素将你挖出的洞填回去。"

# 建筑效果
msgctxt "STRINGS.BUILDINGS.PREFABS.BUILDABLENATURALTILEFIX_NATURALTILE.EFFECT"
msgid "Creates a natural tile from the selected material."
msgstr "用选定的材料创建一个自然地砖。"

# 设置的分类
msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.GENERAL"
msgid "General Settings"
msgstr "常规设置"

# 配置项：建造质量
msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.BUILDMASS.NAME"
msgid "Build Mass"
msgstr "建造质量"

msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.BUILDMASS.DESC"
msgid "Mass required to build the natural tile."
msgstr "建造自然地砖所需的材料质量。"

# 配置项：砖块质量
msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.BLOCKMASS.NAME"
msgid "Block Mass"
msgstr "砖块质量"

msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.BLOCKMASS.DESC"
msgid "Mass of the resulting natural tile."
msgstr "生成的自然地砖的质量。"

# 配置项：建造速度
msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.BUILDSPEED.NAME"
msgid "Build Speed"
msgstr "建造速度"

msgctxt "BuildableNaturalTileFix.NaturalTileStrings.CONFIG.BUILDSPEED.DESC"
msgid "Construction time for the natural tile."
msgstr "自然地砖的建造时间。"
